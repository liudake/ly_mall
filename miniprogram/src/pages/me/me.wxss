.ly-me-header {
  position: relative;
  padding: 60rpx 20rpx 0;
  width: 100%;
  height: 180rpx;
  background-image: url('data:image/jpeg;base64,');
  background-repeat: no-repeat;
  background-size: 100% 100%;
}
.ly-me-user-info {
  display: flex;
}
.ly-me-user-circle {
  display: block;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  border: 4rpx solid #fff;
}
.ly-me-user-detail {
  display: flex;
  -webkit-box-orient: vertical;
  flex-direction: column;
  align-items: flex-start;
  justify-content: center;
  margin-left: 20rpx;
}
.ly-me-user-name {
  font-size: 32rpx;
}
.ly-me-order {
  position: relative;
  width: 100%;
  background-color: #fff;
  margin-bottom: 10px;
}
.ly-me-order-title {
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 20rpx;
  border-bottom: 1rpx solid #f3f3f3;
  box-sizing: border-box;
}
.ly-me-order-all {
  display: block;
  font-size: 26rpx;
  color: #989898;
}
.ly-me-order-list {
  display: flex;
  height: 160rpx;
}
.ly-me-order-item {
  flex: 1;
  display: flex;
  height: 100%;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  overflow: hidden;
}
